{
    "name": "jmhc/hyperf-api",
    "description": "Hyperf restful api.",
    "keywords": [
        "jmhc",
        "restful",
        "api",
        "hyperf"
    ],
    "homepage": "http://jumihc.com/",
    "license": "MIT",
    "authors": [
        {
            "name": "jumihc",
            "email": "jumihuchuang@126.com"
        }
